<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/admin_profile"
    app:startDestination="@+id/navigation_admin_profile">

    <fragment
        android:id="@+id/navigation_admin_profile"
        android:name="com.example.cateringapp.ui.admin.profile.AdminProfileFragment"
        android:label="@string/business_info"
        tools:layout="@layout/fragment_admin_profile" >
        <action
            android:id="@+id/action_navigation_admin_profile_to_navigation_edit_business"
            app:destination="@id/navigation_edit_business" />
    </fragment>

    <fragment
        android:id="@+id/navigation_edit_business"
        android:name="com.example.cateringapp.ui.admin.profile.BusinessEditFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_business_edit" >
    </fragment>
</navigation>